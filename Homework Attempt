#include <iostream>
#include <ctime>
#include <iomanip>
using namespace std;

int main()
{
	int array[10][10] = { 0 };
	for (int i = 0; i < 10; i++) //i is row number
	{
		array[i][0] = i + 1;
	}
	srand(time(0));
	for (int i = 0; i < 10; i++) //i is row number
	{
		for (int j = 1; j < 10; j++) //j is column number
		{
			array[i][j] = 1 + rand() % (25 - 1 + 1);
		}
	}
	cout << "Creating Random Number Connections to Routers 1-10..." << endl;
	for (int i = 0; i < 10; i++) //i is row number
	{
		for (int j = 0; j < 10; j++) //j is column number
		{
			cout << right << setw(3) << array[i][j];
		}
		cout << endl;
	}
	return 0;
}
